% \begin{macro}{\sectionmark}
% Setup space rather than period separated section marks.
% (\file{classes.dtx}, L2054)
%    \begin{macrocode}
\renewcommand{\sectionmark}[1]{%
  \markright {%
    \ifnum \c@secnumdepth >\z@
      \thesection \ %
    \fi
    #1}}
%    \end{macrocode}
% \end{macro}
